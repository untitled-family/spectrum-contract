    function getDetail(uint256 _d)
        external
        pure
        returns (string memory, string memory)
    {
        string memory detail;
        string memory name;

        if (_d < 2) {
            detail = "";
            name = "Perfect";
        } else if (_d < 4) {
            detail = impossible;
            name = "Impossible";
        } else if (_d < 8) {
            detail = legendary;
            name = "Legendary";
        } else if (_d < 15) {
            detail = epicTriple;
            name = "Epic Triple";
        } else if (_d < 23) {
            detail = epicDouble;
            name = "Epic Double";
        } else if (_d < 29) {
            detail = epicSingle;
            name = "Epic Single";
        } else if (_d < 38) {
            detail = rareTriple;
            name = "Rare Triple";
        } else if (_d < 47) {
            detail = rareDouble;
            name = "Rare Double";
        } else if (_d < 56) {
            detail = rareSingle;
            name = "Rare Single";
        } else if (_d < 68) {
            detail = commonTriple;
            name = "Common Triple";
        } else if (_d < 80) {
            detail = commonDouble;
            name = "Common Double";
        } else if (_d < 92) {
            detail = commonSingle;
            name = "Common Single";
        }

        return (
            string.concat(
                '<g style="mix-blend-mode:difference">',
                detail,
                "</g>"
            ),
            name
        );
    }
